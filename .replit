[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask Backend"

[[workflows.workflow]]
name = "Flask Backend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend && python app.py"
waitForPort = 8000

[workflows.workflow.metadata]
outputType = "console"

